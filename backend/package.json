{
  "name": "oneai",
  "version": "1.0.0",
  "description": "One AI - 모든 AI 서비스를 하나로 통합하는 플랫폼",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "dev:watch": "nodemon --watch backend --ext js,json backend/server.js",
    "build": "npm run build:frontend",
    "build:frontend": "node scripts/build.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint backend/ --ext .js",
    "lint:fix": "eslint backend/ --ext .js --fix",
    "format": "prettier --write \"backend/**/*.js\" \"frontend/**/*.{html,css,js}\"",
    "db:migrate": "node backend/migrations/migrate.js",
    "db:seed": "node backend/seeds/seed.js",
    "deploy": "npm run build && node scripts/deploy.js",
    "docker:build": "docker build -t oneai .",
    "docker:run": "docker run -p 3000:3000 oneai"
  },
  "keywords": [
    "ai",
    "artificial-intelligence",
    "chatgpt",
    "claude",
    "gemini",
    "midjourney",
    "ai-platform",
    "ai-integration",
    "ai-sharing",
    "ai-community",
    "ai-business"
  ],
  "author": {
    "name": "OneAI Team",
    "email": "contact@oneai.com",
    "url": "https://oneai.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oneai/oneai.git"
  },
  "bugs": {
    "url": "https://github.com/oneai/oneai/issues"
  },
  "homepage": "https://oneai.com",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "redis": "^4.6.10",
    "mysql2": "^3.6.5",
    "sequelize": "^6.35.1",
    "mongoose": "^8.0.3",
    "prisma": "^5.7.1",
    "@prisma/client": "^5.7.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "aws-sdk": "^2.1508.0",
    "@aws-sdk/client-s3": "^3.456.0",
    "@aws-sdk/s3-request-presigner": "^3.456.0",
    "stripe": "^14.7.0",
    "paypal-rest-sdk": "^1.8.1",
    "toss-payments": "^1.5.0",
    "iamport": "^0.8.1",
    "nodemailer": "^6.9.7",
    "@sendgrid/mail": "^7.7.0",
    "socket.io": "^4.7.4",
    "ws": "^8.14.2",
    "ioredis": "^5.3.2",
    "node-cron": "^3.0.3",
    "agenda": "^5.0.0",
    "axios": "^1.6.2",
    "openai": "^4.20.1",
    "@anthropic-ai/sdk": "^0.9.1",
    "@google-ai/generativelanguage": "^2.5.0",
    "google-auth-library": "^9.4.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "joi": "^17.11.0",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "crypto-js": "^4.2.0",
    "qrcode": "^1.5.3",
    "csv-parser": "^3.0.0",
    "xlsx": "^0.18.5",
    "pdf-parse": "^1.1.1",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.6.1",
    "playwright": "^1.40.1",
    "express-swagger-generator": "^1.1.17",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "babel-jest": "^29.7.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^1.7.1",
    "prettier": "^3.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "cross-env": "^7.0.3",
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0",
    "rimraf": "^5.0.5",
    "npm-run-all": "^4.1.5",
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/qrcode": "^1.5.5",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.1",
    "nyc": "^15.1.0",
    "faker": "^6.6.6",
    "@faker-js/faker": "^8.3.1",
    "artillery": "^2.0.3",
    "docker-compose": "^0.24.6"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "backend/**/*.js",
      "!backend/node_modules/**",
      "!backend/coverage/**",
      "!backend/migrations/**",
      "!backend/seeds/**"
    ],
    "testMatch": [
      "**/tests/**/*.test.js",
      "**/tests/**/*.spec.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2022": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended",
      "plugin:security/recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "error",
      "prefer-const": "error",
      "no-var": "error"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{html,css,json,md}": [
      "prettier --write"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "backend/"
    ],
    "ext": "js,json",
    "ignore": [
      "node_modules/",
      "coverage/",
      "logs/"
    ],
    "delay": 2000
  },
  "config": {
    "mongodbMemoryServer": {
      "version": "7.0.0"
    }
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/oneai"
  },
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}