    <!-- JavaScript -->
    <script>
        // ===== 전역 상태 관리 =====
        window.OneAIApp = {
            currentPage: 'home',
            user: null,
            isAuthenticated: false,
            
            // 초기화
            init() {
                this.checkAuth();
                this.setupEventListeners();
                this.loadInitialPage();
            },
            
            // 인증 확인
            checkAuth() {
                const token = localStorage.getItem('oneai_token');
                const userData = localStorage.getItem('oneai_user');
                
                if (token && userData) {
                    this.user = JSON.parse(userData);
                    this.isAuthenticated = true;
                    this.updateUserUI();
                } else {
                    this.isAuthenticated = false;
                    this.updateLoginUI();
                }
            },
            
            // 사용자 UI 업데이트
            updateUserUI() {
                const userProfile = document.getElementById('userProfile');
                const userAvatar = document.getElementById('userAvatar');
                const userName = document.getElementById('userName');
                const userStatus = document.getElementById('userStatus');
                const loginButton = document.getElementById('loginButton');
                const logoutButton = document.getElementById('logoutButton');
                
                if (this.user) {
                    userProfile.style.display = 'flex';
                    loginButton.style.display = 'none';
                    logoutButton.style.display = 'inline-flex';
                    
                    userAvatar.textContent = this.user.name ? this.user.name.charAt(0).toUpperCase() : 'U';
                    userName.textContent = this.user.name || '사용자';
                    userStatus.textContent = '온라인';
                    
                    // 프로필 페이지 정보 업데이트
                    const profileElements = document.querySelectorAll('[data-user-info]');
                    profileElements.forEach(element => {
                        const field = element.getAttribute('data-user-info');
                        element.textContent = this.user[field] || '';
                    });
                }
            },
            
            // 로그인 UI 업데이트
            updateLoginUI() {
                const userProfile = document.getElementById('userProfile');
                const loginButton = document.getElementById('loginButton');
                const logoutButton = document.getElementById('logoutButton');
                
                userProfile.style.display = 'none';
                loginButton.style.display = 'inline-flex';
                logoutButton.style.display = 'none';
            },
            
            // 이벤트 리스너 설정
            setupEventListeners() {
                // 뒤로가기/앞으로가기 처리
                window.addEventListener('popstate', (event) => {
                    const page = event.state?.page || 'home';
                    this.showPage(page, false);
                });
                
                // 키보드 단축키
                document.addEventListener('keydown', (event) => {
                    if (event.ctrlKey || event.metaKey) {
                        switch(event.key) {
                            case '1':
                                event.preventDefault();
                                this.navigateTo('home');
                                break;
                            case '2':
                                event.preventDefault();
                                this.navigateTo('business');
                                break;
                            case '3':
                                event.preventDefault();
                                this.navigateTo('healthcare');
                                break;
                            case '4':
                                event.preventDefault();
                                this.navigateTo('community');
                                break;
                            case '5':
                                event.preventDefault();
                                this.navigateTo('sharing');
                                break;
                        }
                    }
                });
            },
            
            // 초기 페이지 로드
            loadInitialPage() {
                const hash = window.location.hash.slice(1);
                const page = hash || 'home';
                this.showPage(page, false);
            },
            
            // 페이지 네비게이션
            navigateTo(page) {
                this.showPage(page, true);
            },
            
            // 페이지 표시
            showPage(page, updateHistory = true) {
                // 인증이 필요한 페이지 체크
                const authRequiredPages = ['profile', 'business', 'healthcare', 'community', 'sharing'];
                if (authRequiredPages.includes(page) && !this.isAuthenticated) {
                    this.showPage('login', updateHistory);
                    return;
                }
                
                // 현재 활성 페이지 숨기기
                const currentPage = document.querySelector('.page.active');
                if (currentPage) {
                    currentPage.classList.remove('active');
                }
                
                // 새 페이지 표시
                const newPage = document.getElementById(page + 'Page');
                if (newPage) {
                    newPage.classList.add('active');
                    this.currentPage = page;
                    
                    // 네비게이션 탭 업데이트
                    this.updateNavTabs(page);
                    
                    // 브라우저 히스토리 업데이트
                    if (updateHistory) {
                        const url = page === 'home' ? '/' : `/#${page}`;
                        history.pushState({ page }, '', url);
                    }
                    
                    // 페이지별 초기화
                    this.initPage(page);
                }
            },
            
            // 네비게이션 탭 업데이트
            updateNavTabs(page) {
                const tabs = document.querySelectorAll('.nav-tab');
                tabs.forEach(tab => {
                    tab.classList.remove('active');
                    if (tab.getAttribute('data-page') === page) {
                        tab.classList.add('active');
                    }
                });
            },
            
            // 페이지별 초기화
            initPage(page) {
                switch(page) {
                    case 'home':
                        this.initHomePage();
                        break;
                    case 'business':
                        this.initBusinessPage();
                        break;
                    case 'healthcare':
                        this.initHealthcarePage();
                        break;
                    case 'community':
                        this.initCommunityPage();
                        break;
                    case 'sharing':
                        this.initSharingPage();
                        break;
                    case 'profile':
                        this.initProfilePage();
                        break;
                }
            },
            
            // 홈 페이지 초기화
            initHomePage() {
                // 애니메이션 효과
                const cards = document.querySelectorAll('#homePage .ai-service-card');
                cards.forEach((card, index) => {
                    card.style.animationDelay = `${index * 100}ms`;
                    card.classList.add('slide-up');
                });
            },
            
            // 비즈니스 페이지 초기화
            initBusinessPage() {
                console.log('비즈니스 페이지 초기화');
            },
            
            // 헬스케어 페이지 초기화
            initHealthcarePage() {
                console.log('헬스케어 페이지 초기화');
            },
            
            // 커뮤니티 페이지 초기화
            initCommunityPage() {
                console.log('커뮤니티 페이지 초기화');
            },
            
            // 쉐어링 페이지 초기화
            initSharingPage() {
                console.log('쉐어링 페이지 초기화');
            },
            
            // 프로필 페이지 초기화
            initProfilePage() {
                if (this.user) {
                    this.updateUserUI();
                }
            },
            
            // 로그아웃
            logout() {
                localStorage.removeItem('oneai_token');
                localStorage.removeItem('oneai_user');
                this.user = null;
                this.isAuthenticated = false;
                this.updateLoginUI();
                this.navigateTo('home');
                this.showToast('로그아웃되었습니다.', 'info');
            },
            
            // 토스트 메시지 표시
            showToast(message, type = 'info') {
                // 기존 토스트 제거
                const existingToast = document.querySelector('.toast');
                if (existingToast) {
                    existingToast.remove();
                }
                
                // 새 토스트 생성
                const toast = document.createElement('div');
                toast.className = `toast toast-${type}`;
                toast.style.cssText = `
                    position: fixed;
                    top: 20px;
                    right: 20px;
                    background: var(--color-surface-modal);
                    color: var(--text-primary);
                    padding: var(--spacing-md) var(--spacing-lg);
                    border-radius: var(--radius-md);
                    box-shadow: var(--shadow-xl);
                    z-index: var(--z-index-toast);
                    animation: slideInRight 0.3s ease-out;
                    border-left: 4px solid var(--color-${type === 'error' ? 'error' : type === 'success' ? 'success' : 'info'});
                `;
                toast.textContent = message;
                
                document.body.appendChild(toast);
                
                // 3초 후 제거
                setTimeout(() => {
                    if (toast.parentNode) {
                        toast.style.animation = 'slideOutRight 0.3s ease-out';
                        setTimeout(() => toast.remove(), 300);
                    }
                }, 3000);
            }
        };
        
        // ===== 로그인 처리 =====
        async function handleLogin(event) {
            event.preventDefault();
            
            const email = document.getElementById('email').value;
            const password = document.getElementById('password').value;
            const errorMsg = document.getElementById('errorMessage');
            const submitBtn = document.getElementById('loginSubmitBtn');
            
            try {
                submitBtn.disabled = true;
                submitBtn.classList.add('loading');
                submitBtn.textContent = '로그인 중...';
                errorMsg.style.display = 'none';
                
                // API 호출 (실제 구현에서는 api.js의 OneAIClient 사용)
                const response = await fetch('/api/auth/login', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({ email, password })
                });
                
                const data = await response.json();
                
                if (response.ok && data.success) {
                    // 로그인 성공
                    localStorage.setItem('oneai_token', data.token);
                    localStorage.setItem('oneai_user', JSON.stringify(data.user));
                    
                    OneAIApp.user = data.user;
                    OneAIApp.isAuthenticated = true;
                    OneAIApp.updateUserUI();
                    OneAIApp.showToast('로그인되었습니다!', 'success');
                    OneAIApp.navigateTo('home');
                } else {
                    // 로그인 실패
                    errorMsg.textContent = data.message || '로그인에 실패했습니다.';
                    errorMsg.style.display = 'block';
                }
            } catch (error) {
                console.error('로그인 오류:', error);
                errorMsg.textContent = '네트워크 오류가 발생했습니다.';
                errorMsg.style.display = 'block';
            } finally {
                submitBtn.disabled = false;
                submitBtn.classList.remove('loading');
                submitBtn.textContent = '로그인';
            }
        }
        
        // ===== 전역 함수들 =====
        function navigateTo(page) {
            OneAIApp.navigateTo(page);
        }
        
        function logout() {
            OneAIApp.logout();
        }
        
        // ===== 앱 초기화 =====
        document.addEventListener('DOMContentLoaded', () => {
            OneAIApp.init();
        });
        
        // ===== 애니메이션 CSS 추가 =====
        const style = document.createElement('style');
        style.textContent = `
            @keyframes slideInRight {
                from { transform: translateX(100%); opacity: 0; }
                to { transform: translateX(0); opacity: 1; }
            }
            
            @keyframes slideOutRight {
                from { transform: translateX(0); opacity: 1; }
                to { transform: translateX(100%); opacity: 0; }
            }
        `;
        document.head.appendChild(style);
    </script>
</body>
</html>
