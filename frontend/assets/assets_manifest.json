    "fonts": {
      "inter": {
        "description": "Inter 폰트 패밀리",
        "formats": ["WOFF2", "WOFF", "TTF"],
        "weights": [300, 400, 500, 600, 700, 800],
        "usage": "기본 UI 폰트"
      },
      "jetbrains-mono": {
        "description": "JetBrains Mono (코드용)",
        "formats": ["WOFF2", "WOFF"],
        "weights": [400, 500, 700],
        "usage": "코드 블록, 터미널"
      }
    },
    
    "data": {
      "ai-services.json": {
        "description": "AI 서비스 메타데이터",
        "content": "서비스 정보, URL, 가격, 기능"
      },
      "countries.json": {
        "description": "국가 코드 및 이름 데이터",
        "content": "국제화 지원용"
      },
      "languages.json": {
        "description": "지원 언어 목록",
        "content": "다국어 지원용"
      },
      "pricing-tiers.json": {
        "description": "요금제 정보",
        "content": "구독 플랜, 가격, 기능"
      }
    }
  },
  
  "build": {
    "optimization": {
      "css": {
        "minify": true,
        "autoprefixer": true,
        "critical": true
      },
      "js": {
        "minify": true,
        "treeshake": true,
        "bundle": true
      },
      "images": {
        "compress": true,
        "webp": true,
        "lazy": true
      }
    },
    
    "bundling": {
      "css": {
        "core.css": [
          "themes.css",
          "animations.css",
          "icons.css"
        ],
        "vendor.css": [
          "normalize.css"
        ]
      },
      "js": {
        "core.js": [
          "images.js",
          "sounds.js"
        ],
        "utils.js": [
          "helpers.js",
          "validators.js"
        ]
      }
    }
  },
  
  "cdn": {
    "primary": "https://cdn.oneai.com/assets/",
    "fallback": "https://backup-cdn.oneai.com/assets/",
    "version": "v1.0.0",
    "cacheTTL": "30d"
  },
  
  "preload": {
    "critical": [
      "css/themes.css",
      "css/icons.css",
      "fonts/inter-400.woff2",
      "images/oneai-logo.svg"
    ],
    "important": [
      "css/animations.css",
      "js/images.js",
      "js/sounds.js"
    ],
    "optional": [
      "fonts/jetbrains-mono-400.woff2",
      "images/illustrations/welcome-illustration.svg"
    ]
  },
  
  "accessibility": {
    "reducedMotion": {
      "supported": true,
      "fallbacks": "static equivalents"
    },
    "highContrast": {
      "supported": true,
      "theme": "high-contrast"
    },
    "screenReader": {
      "altTexts": "comprehensive",
      "ariaLabels": "complete"
    }
  },
  
  "performance": {
    "budget": {
      "css": "50KB",
      "js": "100KB",
      "images": "500KB",
      "fonts": "200KB"
    },
    "compression": {
      "gzip": true,
      "brotli": true
    },
    "caching": {
      "strategy": "cache-first",
      "duration": "1y"
    }
  },
  
  "development": {
    "hotReload": true,
    "sourceMap": true,
    "linting": {
      "css": "stylelint",
      "js": "eslint"
    },
    "testing": {
      "visual": "chromatic",
      "unit": "jest"
    }
  },
  
  "deployment": {
    "environments": ["development", "staging", "production"],
    "versioning": "semantic",
    "rollback": "automatic",
    "monitoring": {
      "performance": true,
      "errors": true,
      "usage": true
    }
  },
  
  "license": {
    "type": "MIT",
    "attribution": "OneAI Team",
    "thirdParty": {
      "Inter": "SIL Open Font License",
      "Lucide Icons": "ISC License"
    }
  }
}